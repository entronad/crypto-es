{
  "name": "crypto-es",
  "version": "3.0.0",
  "description": "A cryptography algorithms library compatible with ES6 and TypeScript",
  "keywords": [
    "typescript",
    "security",
    "crypto",
    "cipher",
    "ArrayBuffer",
    "TypedArray",
    "file",
    "ECMAScript",
    "ES6",
    "Hash",
    "MD5",
    "SHA1",
    "SHA-1",
    "SHA2",
    "SHA-2",
    "SHA3",
    "SHA-3",
    "SHA256",
    "SHA-256",
    "RC4",
    "Rabbit",
    "AES",
    "DES",
    "3DES",
    "TripleDES",
    "PBKDF2",
    "HMAC",
    "HEX",
    "Base64",
    "Base64url",
    "blowfish"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:verbose": "sh -c 'tsc 2>&1'",
    "typecheck": "tsc --noEmit",
    "typecheck:verbose": "sh -c 'tsc --noEmit 2>&1'",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:verbose": "sh -c 'node --experimental-vm-modules node_modules/jest/bin/jest.js 2>&1'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entronad/crypto-es.git"
  },
  "author": "LIN Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entronad/crypto-es/issues"
  },
  "homepage": "https://github.com/entronad/crypto-es#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "29.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "testRegex": "(/__tests__/.*\\.test\\.ts)$",
    "preset": "ts-jest/presets/default-esm"
  }
}
